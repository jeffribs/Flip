<h1>Add a project update!</h1>

<%= form_for @post do |f| %>
    <fieldset>
        <div>
            <%= f.label :title %>
            <%= f.text_field :title %>
        </div>
        <div>
            <%= f.label :description %>
            <%= f.text_field :description %>
        </div>
    </fieldset>
    <%= f.submit %>

<% end %>